{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "barryvdh/laravel-debugbar": "^3.5",
        "beyondcode/laravel-dump-server": "^1.7",
        "fideloper/proxy": "^4.4",
        "filp/whoops": "^2.11",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "7.3.0",
        "helmesvs/laravel-notify": "^1.1",
        "intervention/image": "^2.5",
        "ixudra/curl": "^6.21",
        "laravel/framework": "8.0",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maximebf/debugbar": "^1.16",
        "nesbot/carbon": "^2.46",
        "spatie/laravel-activitylog": "^3.17",
        "stripe/stripe-php": "^7.76",
        "sven/artisan-view": "^3.4",
        "yajra/laravel-datatables-buttons": "^4.12",
        "yajra/laravel-datatables-editor": "^1.22",
        "yajra/laravel-datatables-fractal": "^1.6",
        "yajra/laravel-datatables-html": "^4.36"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "maharshiabi/laravel-settings": "dev-master",
        "spatie/laravel-permission": "^3.11",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
